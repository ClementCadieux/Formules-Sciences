<formulas>
    <formula name="Force d'attraction entre 2 corps">
      <var name="q1" unit="C" text="q1"/>
      <var name="q2" unit="C" text="q2"/>
      <var name="r" unit="m" text="r"/>
      <var name="FE" unit="N" text="F.E."/>
      <function name="FE" equation="((9*(10**9))*q1*q2)/(r**2)" text="F.E.=(k⋅q1⋅q2)÷(r^2)"/>
      <function name="q" equation="(FE*(r**2))/(9*(10**9)*q2)" text="q1=(F.E.⋅r^2)÷(k⋅q2)"/>
      <function name="r" equation="math.sqrt((9*(10**9)*q1*q2)/FE)" text="r=√((k⋅q1⋅q2)÷(F.E.))"/>
    </formula>
   <formula name="Tension électrique">
      <var name="U" unit="v" text="U"/>
      <var name="R" unit="Ω" text="R"/>
      <var name="I" unit="A" text="I"/>
      <function name="U" equation="R*I" text=""/>
      <function name="R" equation="U/I" text=""/>
      <function name="I" equation="U/R" text=""/>
   </formula>
   <formula name="Puissance électrique">
	    <var name="U" unit="v" text="U"/>
      <var name="I" unit="A" text="I"/>
	    <var name="P" unit="W" text="P"/>
	    <function name="U" equation="P/I"  text=""/>
    	<function name="I" equation="P/U" text=""/>
     	<function name="P" equation="U*I" text=""/>
    </formula>
    <formula name="Énergie par rapport au temps">
	    <var name="E" unit="j" text="E"/>
	    <var name="dt" unit="s" text="Δt"/>
      <var name="P" unit="W" text="P"/>
	    <function name="E" equation="P*dt"/>
      <function name="dt" equation="E/P"/>
      <function name="P" equation="E/dt"/>
    </formula>
    <formula name="Rapport Résistance-Conductance">
      <var name="G" unit="S" text="G"/>
      <var name="R" unit="Ω" text="R"/>
      <function name="G" equation="R**-1"/>
      <function name="R" equation="G**-1"/>
    </formula>
    <formula name="Best Rap Ever">
      <var name="m" unit="g" text="m"/>
      <var name="dT" unit="degree Celsius" text="ΔT"/>
      <var name="Q" unit="j" text="Q"/>
      <var name="c" unit="j/(g*°C)" text="c"/>
      <function name="Q" equation="m*c*dT"/>
      <function name="m" equation="Q/(c*dT)"/>
      <function name="dT" equation="Q/(c*m)"/>
      <function name="c" equation="Q/(m*dT)"/>
    </formula>
    <formula name="Rapport entre la tension, l'énergie et la charge">
      <var name="E" unit="j" text="E"/>
      <var name="U" unit="v" text="U"/>
      <var name="q" unit="C" text="q"/>
      <function name="E" equation="U/q"/>
      <function name="U" equation="E*q"/>
      <function name="q" equation="U/E"/>
    </formula>
  <formula name="Rendement énergétique">
      <var name="E" unit="j" text="E"/>
      <var name="Re" unit="%" text="R.E."/>
      <var name="Q" unit="j" text="Q"/>
      <function name="E" equation="Q/(Re/100)"/>
      <function name="Re" equation="(Q/E)*100"/>
      <function name="Q" equation="(Re/100)*E"/>
    </formula>
  <formula name="Conversion Coulombs-Électrons">
      <var name="q" unit="C" text="q"/>
      <var name="Ne" unit="e-" text="Electrons"/>
      <function name="q" equation="Ne/(6.242*10**18)"/>
      <function name="Ne" equation="q*(6.242*10**18)"/>
  </formula>
  <formula name="Rapport Charge-Intensité-Temps">
      <var name="I" unit="A" text="I"/>
      <var name="q" unit="C" text="q"/>
      <var name="dt" unit="s" text="Δt"/>
      <function name="I" equation="q/dt"/>
      <function name="q" equation="I*dt"/>
      <function name="dt" equation="q/I"/>
  </formula>
</formulas>
