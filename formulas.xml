  <formula name="Rendement énergétique">
      <var name="E" unit="J" text="E"/>
      <var name="Re" unit="%" text="R.E."/>
      <var name="Q" unit="J" text="Q"/>
      <function name="E" equation="Q/(Re/100)" text="E=Q÷(R.E.÷100)"/>
